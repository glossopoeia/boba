
rec data List x
    = Cons :
        {(List {a vd sd}) vl sl}
        {a v s}
            ;
        (List {a (v and vd) (s or sd)})
    | Nil  : ; (List {a v s})

rec func twice-cons = Cons Cons

rec func list-len l =
    l match {
        (Cons t h) => t list-len inc-i32;
        Nil => 0;
    }

main = Nil 1 Cons 2 Cons list-len