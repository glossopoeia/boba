
rec data List x
    = Cons :
        {(List {a sd}) sl}
        {a s}
            ;
        (List {a (s or sd)})
    | Nil  : ; (List {a s})

rec func twice-cons = Cons Cons

rec func list-len l =
    l match {
        (Cons t h) => t list-len inc-i32;
        Nil => 0;
    }

main = Nil 1 Cons 2 Cons list-len