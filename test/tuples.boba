func ext r = tuple { ...r with false, 2, 1.2 }

func fst =
    match {
        | tuple { b... a } => a ;
    }

func snd =
    match {
        | tuple { c... b a } => b ;
    }

func fstFst =
    match {
        | tuple { b... tuple { c... a } } => a ;
    }

func tail =
    match {
        | tuple { b... a } => tuple { ... b } ;
    }

