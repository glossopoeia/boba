
effect state! s
    = get! : ===[ e..., (state! s) ][ p... ][ True ]==> s
    | put! : s ===[ e..., (state! s) ][ p... ][ True ]==>

func counter =
    get! 1 add-inative
    put!
    get! 1 add-inative
    put!
    get! 1 add-inative

main =
    2
    handle s {
        counter
    } with {
        | get! => s s resume
        | put! n => n resume
        | after r => [s, r]
    }
    head-list