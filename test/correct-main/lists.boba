
func nil = [ ]

func cons x y = [ x... with y ]

func break-list =
    match {
        | [ b... a ] => [ b... ] a
    }

func one-elem = [ 2 ] 2

func with-data = [ True, False, True ]

func ext r = [ r... with False, True, False ]

func fst =
    match {
        | [ b... a ] => a
    }

func snd =
    match {
        | [ c... b a ] => b
    }

func fstFst =
    match {
        | [ b... [ c... a ] ] => a
    }

func tail =
    match {
        | [ b... a ] => [ b... ]
    }

func print-list =
    "(" clear-string print-string
    for e in list then {
        e print-string
        ", " clear-string print-string
    }
    ")" clear-string print-string

func print-two-list =
    "(" clear-string print-string
    for el in list, er in list then {
        el print-string
        ";" clear-string print-string
        er print-string
        ", " clear-string print-string
    }
    ")" clear-string print-string

func map-list f =
    for e in list as list then {
        e f do
    }

func all-list =
    for e in list result res = True then {
        res e and-bool
    }

func extracting =
    [ 0 ] fst
    [ 2, 0, 1 ] snd
    add-inative

