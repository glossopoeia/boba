
func nil = [ ]

func cons x y = [ x... with y ]

func break-list =
    match {
        | [ b... a ] => [ b... ] a
    }

func one-elem = [ 2 ] 2

func with-data = [ True, False, True ]

func ext r = [ r... with False, True, False ]

func fst =
    match {
        | [ b... a ] => a
    }

func snd =
    match {
        | [ c... b a ] => b
    }

func fstFst =
    match {
        | [ b... [ c... a ] ] => a
    }

func tail =
    match {
        | [ b... a ] => [ b... ]
    }

func print-list =
    "(" print-string
    for e <= list then {
        e print-string
        ", " print-string
    }
    ")" print-string

func print-two-list =
    "(" print-string
    for el <= list, er <= list then {
        el print-string
        ";" print-string
        er print-string
        ", " print-string
    }
    ")" print-string

func map-list f =
    for e <= list map list, list then {
        let x = e f do;
        x x
    }

func dup-list =
    for e <= list map list, list then {
        e e
    }

func all-list =
    for e <= list fold res <= True then {
        res e and-bool
    }

func zip-with f =
    for el <= list, er <= list map list then {
        el er f do
    }

func cmp-list f =
    for el <= list, er <= list fold res <= True then {
        el er f do res and-bool
    }

func extracting =
    [ 0 ] fst
    [ 2, 0, 1 ] snd
    add-i32

