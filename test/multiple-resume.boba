func drop n =

effect amb! {
    flip! : {(--> (amb! | e) p t [] [{Bool true false s}]) true false false}
}

main =
    handle {
        let x = flip!;
        let y = flip!;
        x y xor-bool
    } with {
        flip! => {
            let x = false resume;
            let y = true resume;
            x y append-list
        };
        after v => nil-list v cons-list;
    }
    drop 0