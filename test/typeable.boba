
data Proxy a
    = Proxy :  => (Proxy a)

data TypeRep
    = OneCtor : (String t c)^s => TypeRep
    | TwoCtor : (String t c)^s => TypeRep

overload typeOf as Typeable? a
    : z... (Proxy a)^s ===[ e... ][ p... ][ t ]==> z... TypeRep^r

instance typeOf : Bool
    = "Bool" OneCtor

