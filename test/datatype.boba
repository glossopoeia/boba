
data Boolean
    = BTrue : ; Boolean
    | BFalse : ; Boolean

data Option x
    = Some : {a v s} ; (Option {a v s})
    | None : ; (Option {a v s})

func optMap f =
    match {
        (Some a) => a f do Some;
        None => None;
    }

main =
    0 Some match {
        (Some a) => a;
        None => 1;
    }